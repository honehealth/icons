{
  "name": "@honehealth/icons",
  "version": "3.3.0",
  "author": "Hone Health <info@honehealth.com>",
  "type": "module",
  "main": "lib/index.js",
  "types": "types.d.ts",
  "exports": {
    ".": {
      "dev": "./build/index.jsx",
      "import": {
        "default": "./lib/index.js",
        "types": "./types.d.ts"
      }
    }
  },
  "files": [
    "lib/*",
    "types.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:honehealth/icons.git"
  },
  "packageManager": "pnpm@10.28.2+sha512.41872f037ad22f7348e3b1debbaf7e867cfd448f2726d9cf74c08f19507c31d2c8e7a11525b983febc2df640b5438dee6023ebb1f84ed43cc2d654d2bc326264",
  "engines": {
    "node": ">=24 <25",
    "pnpm": ">=10 <11",
    "npm": ">=11 <12"
  },
  "scripts": {
    "build": "rimraf ./lib && rimraf ./build && pnpm build:icons && pnpm build:react && pnpm build:babel",
    "build:icons": "svgo -f src/icons -o lib",
    "build:react": "svgr --ext jsx --replace-attr-values \"#0E0B20=currentColor\" --replace-attr-values \"#161919=currentColor\" --replace-attr-values \"#202020=currentColor\" -d build ./src/icons",
    "build:babel": "babel build --out-dir lib",
    "prepublish": "pnpm build"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.6",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@svgr/cli": "^8.1.0",
    "rimraf": "^3.0.2",
    "svgo": "^3.0.4"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0"
  }
}
